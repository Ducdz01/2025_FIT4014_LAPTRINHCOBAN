#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    double item1 = 15.59, item2 = 24.59, item3 = 6.59, item4 = 12.59, item5 = 3.59;
    double subtotal = item1 + item2 + item3 + item4 + item5;
    double tax = subtotal * 0.07;
    double total = subtotal + tax;

    cout << "Gia tung mon: " << item1 << ", " << item2 << ", " << item3 << ", " << item4 << ", " << item5 << endl;
    cout << "Tam tinh: " << subtotal << endl;
    cout << "Thue (7%): " << tax << endl;
    cout << "Tong cong: " << total << endl;
    return 0;
}
